{
	"name": "Golang application",
	
	// docker-compose.yaml with context set as git URL
	"dockerComposeFile": "docker-compose.yaml",

	// Service name in docker-compose.yaml
	"service": "app",

	// Workspace folder to open in container
	"workspaceFolder": "/root",

	// Ports to forward from container to host
	"forwardPorts": [8080]
}
